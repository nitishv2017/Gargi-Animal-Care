<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#30475E</item>
        <item name="colorPrimaryDark">#121212</item>
        <item name="colorAccent">#F05454</item>
    </style>

    <style name="Divider">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/table_border</item>
    </style>

    <style name="Divider_2">
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/table_border</item>
    </style>

    <style name="Invisible_Divider">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="customerPageBottomNavStyle">
        <item name="android:textSize">20sp</item>
    </style>
    <style name="TextInputLayoutAppearanceDifferent" parent="@style/ThemeOverlay.MaterialComponents">
        <item name="colorOnSurface">@color/font_</item>
        <item name="colorPrimary">@color/font_</item>
        <item name="colorControlNormal">@color/font_</item>
    </style>

</resources>
